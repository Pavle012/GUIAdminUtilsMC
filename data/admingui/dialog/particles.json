{
  "type": "minecraft:multi_action",
  "title": "Potion effects",
  "body": [
    {
      "type": "minecraft:plain_message",
      "contents": "Set the effect strength to 1 smaller than you want because of the command syntax. (instead of 1 set 0, instead of 2 set 1, etc)",
      "width": 250
    },
    {
      "type": "minecraft:plain_message",
      "contents": "The max you can set is 255"
    }
  ],
  "inputs": [
    {
      "type": "minecraft:boolean",
      "key": "particles",
      "label": "Visible particles",
      "initial": false,
      "on_true": "false",
      "on_false": "true"
    },
    {
      "type": "minecraft:text",
      "key": "length",
      "label": "Effect time"
    },
    {
      "type": "minecraft:text",
      "key": "strength",
      "width": 100,
      "label": "Effect strength",
      "initial": "0",
      "max_length": 3
    },
    {
      "type": "minecraft:text",
      "key": "username",
      "label": "Player name",
      "initial": "@s"
    }
  ],
  "actions": [
    {
      "label": "Set speed",
      "action": {
        "type": "minecraft:dynamic/run_command",
        "template": "effect give $(username) speed $(length) $(strength) $(particles)"
      }
    },
    {
      "label": "Set jump boost",
      "action": {
        "type": "minecraft:dynamic/run_command",
        "template": "effect give $(username) jump_boost $(length) $(strength) $(particles)"
      }
    },
    {
      "label": "Set slowness",
      "action": {
        "type": "minecraft:dynamic/run_command",
        "template": "effect give $(username) slowness $(length) $(strength) $(particles)"
      }
    },
    {
      "label": "Set haste",
      "action": {
        "type": "minecraft:dynamic/run_command",
        "template": "effect give $(username) haste $(length) $(strength) $(particles)"
      }
    },
    {
      "label": "Set weakness",
      "action": {
        "type": "minecraft:dynamic/run_command",
        "template": "effect give $(username) weakness $(length) $(strength) $(particles)"
      }
    },
    {
      "label": "Set strength",
      "action": {
        "type": "minecraft:dynamic/run_command",
        "template": "effect give $(username) strength $(length) $(strength) $(particles)"
      }
    },
    {
      "label": "Set fire resistance",
      "action": {
        "type": "minecraft:dynamic/run_command",
        "template": "effect give $(username) fire_resistance $(length) $(strength) $(particles)"
      }
    },
    {
      "label": "Set water breathing",
      "action": {
        "type": "minecraft:dynamic/run_command",
        "template": "effect give $(username) water_breathing $(length) $(strength) $(particles)"
      }
    },
    {
      "label": "Set invisibility",
      "action": {
        "type": "minecraft:dynamic/run_command",
        "template": "effect give $(username) invisibility $(length) $(strength) $(particles)"
      }
    },
    {
      "label": "Set blindness",
      "action": {
        "type": "minecraft:dynamic/run_command",
        "template": "effect give $(username) blindness $(length) $(strength) $(particles)"
      }
    },
    {
      "label": "Set nausea",
      "action": {
        "type": "minecraft:dynamic/run_command",
        "template": "effect give $(username) nausea $(length) $(strength) $(particles)"
      }
    },
    {
      "label": "Set regeneration",
      "action": {
